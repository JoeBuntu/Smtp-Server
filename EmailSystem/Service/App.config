<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.diagnostics>
    <sources>
      <source name="EmailingSystem.Service" switchValue="All, ActivityTracing">
        <listeners> 
          <add name="xml" type="System.Diagnostics.XmlWriterTraceListener" initializeData="trace.svclog" />
        </listeners>
      </source>
    </sources>
    <trace autoflush="true" />
  </system.diagnostics>
</configuration>